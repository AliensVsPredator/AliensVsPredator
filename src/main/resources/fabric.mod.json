{
	"schemaVersion": 1,
	"id": "avp",
	"version": "${version}",
	"name": "avp",
	"description": "Adds a variety of content from both the Alien and Predator franchises!",
	"authors": [
		"AzureDoom",
		"Boston Vanseghi",
		"Caution",
		"gorblegorble",
		"Gothic Lycan",
		"Hossaurus",
		"Pheonix Atredes",
		"Shift",
		"Spookyboo117",
		"Whiteshark01",
		"Xavier"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/avp/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.avp.AVP"
		],
		"client": [
			"com.avp.client.AVPClient"
		],
		"fabric-datagen": [
			"com.avp.data.AVPDataGenerator"
		]
	},
	"mixins": [
		"avp.mixins.json",
		"avp.server.mixins.json",
		{
			"config": "avp.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*",
		"azurelib": "~3.0.6"
	},
	"suggests": {
		"another-mod": "*"
	}
}
