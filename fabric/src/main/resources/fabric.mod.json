{
	"schemaVersion": 1,
	"id": "avp",
	"version": "${version}",
	"name": "${mod_name}",
	"description": "${description}",
	"accessWidener": "avp.accesswidener",
	"authors": [
		"AzureDoom",
		"Boston Vanseghi",
		"Caution",
		"gorblegorble",
		"Gothic Lycan",
		"Hossaurus",
		"Pheonix Atredes",
		"Shift",
		"Spookyboo117",
		"Whiteshark01",
		"Xavier"
	],
	"contact": {
		"homepage": "https://www.curseforge.com/minecraft/mc-mods/avp-regenesis",
		"sources": "https://github.com/AliensVsPredator/AliensVsPredator"
	},
	"license": "CC0-1.0",
	"icon": "assets/avp/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.avp.fabric.AVPFabric"
		],
		"client": [
			"com.avp.fabric.client.AVPFabricClient"
		],
		"fabric-datagen": [
			"com.avp.fabric.data.AVPDataGenerator"
		]
	},
	"mixins": [
		"avp.mixins.json"
	],
	"depends": {
		"fabricloader": ">=${fabric_loader_version}",
		"fabric-api": "*",
		"minecraft": "${minecraft_version}",
		"java": ">=${java_version}",
		"azurelib": "~3.0.6"
	},
	"suggests": {
		"another-mod": "*"
	}
}
